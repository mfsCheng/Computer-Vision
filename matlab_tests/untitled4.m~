mat_struct = load('obj_db.mat');
obj_db = mat_struct.obj_db;
% img_list = {'many_objects_1', 'many_objects_2'};
% 
% for i = 1:length(img_list)
%     labeled_img = imread(['labeled_' img_list{i} '.png']);
%     orig_img = imread([img_list{i} '.png']);
%     
%     output_img = recognizeObjects(orig_img, labeled_img, obj_db);
%     %imwrite(output_img, ['testing1c1_' img_list{i} '.png']);
% end
orig_img = imread('many_objects_1.png');
labeled_img = imread('labeled_many_objects_1.png');
output_img = recognizeObjects(orig_img, labeled_img, obj_db);

function output_img = recognizeObjects(orig_img, labeled_img, obj_db)
[props, out] = compute2DProperties(orig_img, labeled_img);
count = 0;
labels = zeros(length(props(1, :)),1);
length(props(1, :))
length(obj_db(1, :))

for i = 1 : length(props(1, :))
    for j = 1 : length(obj_db(1, :))
        if abs(props(6, i) - obj_db(6, j)) >= 0.03
            continue;
        end
        if abs(props(7, i) - obj_db(7, j)) >= 300
            continue;
        end
        count = count + 1;
        labels(count) = props(1, i);
    end
end
fh = figure();
imshow(orig_img);
hold on;
for i = 1 : count
    label = labels(i);
    plot( props(2, label), db(3, label), 'ws', 'MarkerFaceColor', [1 0 0]);
    quiver(db(2, i), db(3, i), 20, 20*tan(orien), 'r','LineWidth',1.5);
end

figure(fh); % Shift the focus back to the figure fh
set(fh, 'WindowStyle', 'normal');
output_img = getimage(fh);
end